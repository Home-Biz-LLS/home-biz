datasource db {
  url = env("DB_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "@prisma/client"
}

model User {
  id Int        @id @default(autoincrement())
  createdAt   DateTime   @default(now())
  updatedAt DateTime @updatedAt
  email String @unique
  name String
}

model Plants {
  id Int @id @default(autoincrement())
}